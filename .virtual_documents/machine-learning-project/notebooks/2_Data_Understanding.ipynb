    plt.figure(figsize=(5,5))
    plt.pie([cols_with_nulls, cols_without_nulls], labels=['Nulos','Completos'], autopct='%1.1f%%')
    plt.title(f'Proporción de valores nulos - {year}')
    plt.show()






